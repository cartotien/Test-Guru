<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><%= button_to 'Show', admin_test_path(test), :method => :get %></td>
  <td><%= button_to 'Edit', edit_admin_test_path(test), :method => :get %></td>
  <td><%= button_to 'Delete', admin_test_path(test), :method => :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
