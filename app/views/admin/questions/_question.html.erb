<%= question.body %>
  <%= link_to t('show'), admin_question_path(question), :method => :get, class: "btn btn-secondary btn-sm" %>
  <%= link_to t('edit'), edit_admin_question_path(question), :method => :get, class: "btn btn-secondary btn-sm" %>
  <%= link_to t('delete'), admin_question_path(question), :method => :delete, data: { confirm: t('sure?') }, class: "btn btn-secondary btn-sm" %><br>
<%= render question.answers %>
